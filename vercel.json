{
  "version": 2,
  "builds": [
    {
      "src": "apps/marketing/next.config.ts",
      "use": "@vercel/next"
    },
    {
      "src": "apps/dashboard/next.config.ts",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(dashboard(?:/.*)?)",
      "dest": "apps/dashboard/$1"
    },
    {
      "src": "/(.*)",
      "dest": "apps/marketing/$1"
    }
  ]
}
